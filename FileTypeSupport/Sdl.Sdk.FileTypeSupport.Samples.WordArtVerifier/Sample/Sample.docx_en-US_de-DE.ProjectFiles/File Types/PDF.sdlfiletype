    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Underline"&gt;
      &lt;StartTag&gt;&amp;lt;cf underlinestyle="single"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Subscript"&gt;
      &lt;StartTag&gt;&amp;lt;cf subscript="on"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Superscript"&gt;
      &lt;StartTag&gt;&amp;lt;cf superscript="on"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Custom1"&gt;
      &lt;StartTag&gt;&amp;lt;cf bold="off"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Custom2"&gt;
      &lt;StartTag&gt;&amp;lt;cf italic="off"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Custom3"&gt;
      &lt;StartTag&gt;&amp;lt;cf underlinestyle="none"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
    &lt;/QuickTag&gt;
    &lt;QuickTag Command="Custom4"&gt;
      &lt;StartTag&gt;&amp;lt;cf superscript="off"&amp;gt;&lt;/StartTag&gt;
      &lt;EndTag&gt;&amp;lt;/cf&amp;gt;&lt;/EndTag&gt;
