<!--	
	
	This is a filter definition file for the SDL Trados filter plug-in for 
	Microsoft(r) PowerPoint(tm) files. It allows the adjustment of the default 
	settings of the filter. Possible setting values are described later in 
	the file. Any unauthorised modification of the settings might result in the 
	filter not working correctly.
	
-->
<!-- 	Original file name: 	PowerPoint_2_0_0_0.xml	-->
<!-- 	Version: 				  2.0.15.10	-->
<!-- 	Last change: 			05 Jun 2008	-->
<FilterDefinition>
  <Guid>TRADOS PowerPoint Filter 2.0.0.0</Guid>
  <FileTypeName>Microsoft PowerPoint XP-2003</FileTypeName>
  <FileFilterRegExp>*.ppt;*.pps;*.pot</FileFilterRegExp>
  <QuickTags>
    <QuickTag Command="Bold">
      <StartTag>&lt;cf bold="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Italic">
      <StartTag>&lt;cf italic="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Underline">
      <StartTag>&lt;cf underline="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Subscript">
      <StartTag>&lt;cf baselineoffset="-25" subscript="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Superscript">
      <StartTag>&lt;cf baselineoffset="30" superscript="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom1">
      <StartTag>&lt;cf emboss="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom2">
      <StartTag>&lt;cf shadow="on"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom3">
      <StartTag>&lt;cf bold="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom4">
      <StartTag>&lt;cf italic="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom5">
      <StartTag>&lt;cf underline="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom6">
      <StartTag>&lt;cf baselineoffset="0" subscript="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom7">
      <StartTag>&lt;cf baselineoffset="0" superscript="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom8">
      <StartTag>&lt;cf emboss="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom9">
      <StartTag>&lt;cf shadow="off"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <!--<QuickTag Command="Custom0">
	      <StartTag>&lt;br/&gt;</StartTag>
	      <EndTag>
        </EndTag>
	    </QuickTag>-->
    <QuickTag Command="Custom0">
      <StartTag>* </StartTag>
      <EndTag> *</EndTag>
    </QuickTag>
  </QuickTags>
  <Scanner Instantiation="C++" FactoryFunction="CreatePPTFileScanner">
    <Guid>PowerPointPlugInFilter.dll</Guid>
    <Settings>
    </Settings>
  </Scanner>
  <Parser Instantiation="C++" FactoryFunction="CreatePPTFileParser">
    <Guid>PowerPointPlugInFilter.dll</Guid>
    <Settings>
      <SlidesInPresentationOrder>Excludenotes</SlidesInPresentationOrder>
      <!-- SlidesInPresentationOrder possible values: NoNotes, NotesAfterAllSlides, 
			NotesAfterEverySlide, Default (equal NotesAfterEverySlide) -->
      <ShapesOnSlideOrder>Default</ShapesOnSlideOrder>
      <!-- ShapesOnSlideOrder possible values: TopLeftBottomRightByCol, TopLeftBottomRightByRow 
			TopRightBottomLeftByCol, TopRightBottomLeftByRow, Default (equal TopRightBottomLeftByRow for 
			bidirectional source languages, otherwise TopLeftBottomRightByRow) -->
      <CommentProcessing>Default</CommentProcessing>
      <!-- CommentProcessing applies to comments inserted with PowerPoint 97 or 2000
			CommentProcessing possible values: On, Off, Default (equal Off) -->
      <XPCommentProcessing>Default</XPCommentProcessing>
      <!-- XPCommentProcessing applies to comments inserted with PowerPoint XP, 2003 or newer
			XPCommentProcessing possible values: On, Off, Default (equal Off) -->
      <HyperlinkProcessing>Default</HyperlinkProcessing>
      <!-- HyperlinkProcessing possible values: On, Off, Partial, Default (equal On) -->
      <SegmentationAfterBreakTags>Default</SegmentationAfterBreakTags>
      <!-- SegmentationAfterBreakTags possible values: On, Off, InListsOnly, Default (equal On) -->
      <SegmentationAfterSoftReturnTags>Default</SegmentationAfterSoftReturnTags>
      <!-- SegmentationAfterSoftReturnTags possible values: On, Off, Default (equal Off) -->
      <BreakTagToExternal>Default</BreakTagToExternal>
      <!-- BreakTagToExternal possible values: On, Off, Default (equal On) -->
      <DisplayFormatting>Default</DisplayFormatting>
      <!-- DisplayFormatting possible values: On, Off, Default (equal On) -->
      <ColourAdaptation>Default</ColourAdaptation>
      <!-- ColourAdaptation possible values: Off, AllToBlack, DarkenedLightColours, 
			Default (equal DarkenedLightColours) -->
      <FontAdaptation>Default</FontAdaptation>
      <!-- FontAdaptation possible values: Off, AllToSingleSize, ResizeToVisible, 
			Default (equal ResizeToVisible) -->
      <MinFontSize>Default</MinFontSize>
      <MaxFontSize>Default</MaxFontSize>
      <SingleSize>Default</SingleSize>
      <FontMapping>Default</FontMapping>
      <!-- FontMapping possible values: On, Off, Default (equal On) -->
      <FontMappingRules>
      </FontMappingRules>
    </Settings>
  </Parser>
