              <constructor-arg>
                <value>Sdl.FileTypeSupport.Bilingual.Xliff.OtherSettings, Sdl.FileTypeSupport.Bilingual.Xliff</value>
              </constructor-arg>
            </object>
          </constructor-arg>
          <constructor-arg>
