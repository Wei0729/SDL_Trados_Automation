    <QuickTag Command="Custom3">
      <StartTag>&lt;cf cstyle="[No character style]" capm="e_smcp"&gt;</StartTag>
      <EndTag>&lt;/cf&gt;</EndTag>
    </QuickTag>
    <QuickTag Command="Custom4">
