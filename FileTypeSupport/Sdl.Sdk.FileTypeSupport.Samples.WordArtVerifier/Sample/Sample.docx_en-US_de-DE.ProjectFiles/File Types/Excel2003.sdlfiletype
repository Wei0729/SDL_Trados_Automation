          &lt;!-- DisplayFormatting possible values: On, Off, Default (equal On) --&gt;
        &lt;/Settings&gt;
      &lt;/Component&gt;
      &lt;Component Instantiation="COM"&gt;
        &lt;Guid&gt;Trados.ExcelDisplayFontMapper2&lt;/Guid&gt;
