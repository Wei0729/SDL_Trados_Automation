          &lt;ReductionFactor&gt;Default&lt;/ReductionFactor&gt;
          &lt;SingleSize&gt;Default&lt;/SingleSize&gt;
        &lt;/Settings&gt;
      &lt;/Component&gt;
    &lt;/Components&gt;
  &lt;/Parser&gt;
